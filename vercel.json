{
  "version": 2,
  "builds": [
      {
          "src": "./server.py",
          "use": "@vercel/python",
            "config": { 
                "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ]
}